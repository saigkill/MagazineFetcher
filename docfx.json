{
	"$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
	"metadata": [
		{
			"src": [
				{
					"src": "src",
					"files": [
						"**/*.csproj"
					]
				}
			],
			"dest": "api"
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"**/*.{md,yml}"
				],
				"exclude": [
					"_MagazineFetcher/**",
					"azure-pipelines.yml"
				]
			}
		],
		"resource": [
			{
				"files": [
					"Assets/**"
				]
			}
		],
		"output": "_MagazineFetcher",
		"template": [
			"default",
			"modern"
		],
		"globalMetadata": {
			"_appName": "MagazineFetcher",
			"_appTitle": "MagazineFetcher",
			"_enableSearch": true,
			"pdf": false
		}
	}
}
